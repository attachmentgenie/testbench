{
    "name": "attachmentgenie/testbench",
    "description": "test framework to keep track of troublesome queries\"",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Vogelaar",
            "email": "bram@attachmentgenie.com"
        }
    ],
    "require": {
        "cilex/cilex": "~1.1",
        "ext-mongo": "~1.5",
        "phpunit/phpunit": "~4.8",
        "zumba/mongounit": "~1.2",
        "bylexus/php-prereqcheck": "0.2.*",
        "kevinlebrun/colors.php": "0.4.*",
        "phpunit/dbunit": "~1.3"
    },
    "bin": [
        "testbench"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.1"
    }
}
